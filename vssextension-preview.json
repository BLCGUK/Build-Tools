{
    "manifestVersion": 1,
    "id": "hitachi-ci-build-tools",
    "name": "Hitachi Build Tools",
    "version": "1.0.65",
    "publisher": "hitachisolutionseurope",
    "targets": [
      {
        "id": "Microsoft.VisualStudio.Services"
      }
    ],
    "description": "Tasks for automating PowerPlatform commands from build and release pipelines",
    "categories": [
      "Azure Pipelines"
    ],
    "public": false,
    "galleryFlags": [
